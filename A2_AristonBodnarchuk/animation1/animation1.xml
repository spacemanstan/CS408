<?xml version="1.0" encoding="UTF-8"?>

<!-- 
    animations rendered at 1280 x 720
    rotation is in degrees

    keyframes of same ID must be in ascending order
    keyframes must have an associated object
 -->

<animation>
    <object id="rocket" type="obj">/spaceShip/spaceShip.obj</object>
    <keyframe id="rocket" frame="0">
        <position x="-300" y="360" z="0" />
        <rotation x="135" y="0" z="-90" />
        <scale x="100" y="100" z="100" />
    </keyframe>
    <keyframe id="rocket" frame="60">
        <position x="1500" y="360" z="0" />
        <rotation x="495" y="0" z="-90" />
        <scale x="100" y="100" z="100" />
    </keyframe>
    <keyframe id="rocket" frame="90">
        <position x="1500" y="360" z="-200" />
        <rotation x="495" y="0" z="90" />
        <scale x="100" y="100" z="100" />
    </keyframe>
    <keyframe id="rocket" frame="150">
        <position x="-100" y="360" z="-200" />
        <rotation x="135" y="0" z="90" />
        <scale x="60" y="60" z="60" />
    </keyframe>

    <object id="space" type="img" width="1280" height="720">/textures/space.png</object>
    <keyframe id="space" frame="0">
        <position x="960" y="360" z="-500" />
        <rotation x="5" y="0" z="0" />
        <scale x="3.5" y="3.5" z="3.5" />
    </keyframe>
    <keyframe id="space" frame="150">
        <position x="320" y="360" z="-500" />
        <rotation x="0" y="-5" z="0" />
        <scale x="3" y="3" z="3" />
    </keyframe>

    <object id="earf" type="sphere">/textures/earf.png</object>
    <keyframe id="earf" frame="0">
        <position x="-300" y="360" z="0" />
        <rotation x="135" y="0" z="-90" />
        <scale x="100" y="100" z="100" />
    </keyframe>
    <keyframe id="earf" frame="150">
        <position x="1500" y="360" z="0" />
        <rotation x="495" y="0" z="-90" />
        <scale x="100" y="100" z="100" />
    </keyframe>
</animation>