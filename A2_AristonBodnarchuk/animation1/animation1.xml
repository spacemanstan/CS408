<?xml version="1.0" encoding="UTF-8"?>

<!-- 
    animations rendered at 1280 x 720
    rotation is in degrees

    keyframes of same ID must be in ascending order
    keyframes must have an associated object
 -->

<animation>
    <object id="rocket" type="obj">/spaceShip/spaceShip.obj</object>
    <keyframe id="rocket" frame="0">
        <position x="0" y="360" z="0" />
        <rotation x="135" y="0" z="-90" />
        <scale x="100" y="100" z="100" />
    </keyframe>
    <keyframe id="rocket" frame="60">
        <position x="1280" y="360" z="0" />
        <rotation x="495" y="0" z="-90" />
        <scale x="100" y="100" z="100" />
    </keyframe>
    <keyframe id="rocket" frame="90">
        <position x="1280" y="360" z="0" />
        <rotation x="495" y="0" z="90" />
        <scale x="100" y="100" z="100" />
    </keyframe>
    <keyframe id="rocket" frame="150">
        <position x="0" y="360" z="0" />
        <rotation x="135" y="0" z="90" />
        <scale x="100" y="100" z="100" />
    </keyframe>
</animation>